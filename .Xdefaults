! This file was generated by running:
! % erb .Xdefaults.erb > .Xdefaults

!-----------------------------------------------------------------------------
! appearance
!-----------------------------------------------------------------------------

! see "font4" in /usr/share/X11/app-defaults/XTerm
*font                       : -*-fixed-medium-r-*-*-13-*-*-*-*-*-*-*

! enable colors in native X applications
#ifdef COLOR
*customization              : -color
#endif

! use the Human cursor theme from Ubuntu
Xcursor.theme               : Human

!-----------------------------------------------------------------------------
! Rxvt
!-----------------------------------------------------------------------------

! interaction
Rxvt.saveLines              : 8192
Rxvt.scrollTtyOutput        : False
Rxvt.scrollTtyKeypress      : True
Rxvt.scrollWithBuffer       : True

! emulate XTerm keycodes to support Vim
Rxvt.keysym.M-Up            : \033[1;3A
Rxvt.keysym.M-Down          : \033[1;3B
Rxvt.keysym.M-Right         : \033[1;3C
Rxvt.keysym.M-Left          : \033[1;3D
Rxvt.keysym.M-S-Up          : \033[1;4A
Rxvt.keysym.M-S-Down        : \033[1;4B
Rxvt.keysym.M-S-Right       : \033[1;4C
Rxvt.keysym.M-S-Left        : \033[1;4D
Rxvt.keysym.C-Up            : \033[1;5A
Rxvt.keysym.C-Down          : \033[1;5B
Rxvt.keysym.C-Right         : \033[1;5C
Rxvt.keysym.C-Left          : \033[1;5D
Rxvt.keysym.C-S-Up          : \033[1;6A
Rxvt.keysym.C-S-Down        : \033[1;6B
Rxvt.keysym.C-S-Right       : \033[1;6C
Rxvt.keysym.C-S-Left        : \033[1;6D
Rxvt.keysym.M-Page_Up       : \033[5;3~
Rxvt.keysym.M-Page_Down     : \033[6;3~
Rxvt.keysym.M-Home          : \033[1;3H
Rxvt.keysym.M-End           : \033[1;3F
Rxvt.keysym.M-S-Home        : \033[1;4H
Rxvt.keysym.M-S-End         : \033[1;4F
Rxvt.keysym.C-Page_Up       : \033[5;5~
Rxvt.keysym.C-Page_Down     : \033[6;5~
Rxvt.keysym.C-Home          : \033[1;5H
Rxvt.keysym.C-End           : \033[1;5F
Rxvt.keysym.C-S-Home        : \033[1;6H
Rxvt.keysym.C-S-End         : \033[1;6F

! appearance
Rxvt.font                   : -*-tamsyn-medium-*-*-*-15-*-*-*-*-*-*-*
Rxvt.scrollBar_right        : True
Rxvt.scrollBar_floating     : True
Rxvt.scrollstyle            : plain

! extensions
URxvt.perl-ext-common       : default,matcher
URxvt.urlLauncher           : /usr/bin/xdg-open
URxvt.matcher.button        : 2

!-----------------------------------------------------------------------------
! XTerm
!-----------------------------------------------------------------------------

! interaction
XTerm*vt100.metaSendsEscape : True
XTerm*vt100.saveLines       : 8192

! appearance
XTerm*font                  : -*-tamsyn-medium-*-*-*-15-*-*-*-*-*-*-*
XTerm*vt100.colorMode       : True
XTerm*vt100.colorBDMode     : True
XTerm*vt100.colorULMode     : True
XTerm*vt100.showBlinkAsBold : True
XTerm*vt100.veryBoldColors  : 6

!-----------------------------------------------------------------------------
