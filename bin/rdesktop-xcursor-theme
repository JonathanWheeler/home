#!/bin/sh -eu
#
# Applies the given xcursor theme to rdesktop(1), after restart.
#
# Usage: rdesktop-install-cursors /usr/share/icons/THEME/cursors
# Where: THEME is the name of the xcursor theme you want to use
#
# The cursors herein were discovered by running rdesktop(1) under
# the XCURSOR_DISCOVER=1 environment and connecting to Windows 7.
#
# Inspired by https://github.com/DavidLP/rdesktop_cursor_fix and
# https://sourceforge.net/p/enlightenment/mailman/message/9320868/
#
# Written in 2017 by Suraj N. Kurapati <https://github.com/sunaku>

SOURCE=$1
TARGET=~/.icons/default/cursors
mkdir -p "$TARGET"

sed '1,/^exit$/d; s/#.*//; /^ *$/d' "$0" |
while read hash name; do
  src="$SOURCE/$name" dst="$TARGET/$hash"
  test -e "$src" && ln -vsf "$src" "$dst"
done

exit

# __        ___           _                     _____      _
# \ \      / (_)_ __   __| | _____      _____  |___  |    / \   ___ _ __ ___
#  \ \ /\ / /| | '_ \ / _` |/ _ \ \ /\ / / __|    / /    / _ \ / _ \ '__/ _ \
#   \ V  V / | | | | | (_| | (_) \ V  V /\__ \   / /    / ___ \  __/ | | (_) |
#    \_/\_/  |_|_| |_|\__,_|\___/ \_/\_/ |___/  /_/    /_/   \_\___|_|  \___/
#

